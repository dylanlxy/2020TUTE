l = []
while 1:
    b = input('Please input your information:')
    if b == 'q':
        break
    else:
        l.append(b)
def funcrepeat(l):
    if list(set（l)) == l:
        print('您输入的值重复：')
    else:
        print('继续输入：')
    print(l)
funcrepeat(l)








